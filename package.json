{
  "name": "latamtcg",
  "version": "0.9.0",
  "private": true,
  "scripts": {
    "dev": "NEXT_FORCE_WEBPACK=1 next dev",
    "build": "NEXT_FORCE_WEBPACK=1 next build",
    "start": "next start",
    "perf:prod": "next build && node scripts/perf-prod.js",
    "lint": "eslint",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name mtg_paper_lang_filters",
    "scryfall:download": "NODE_OPTIONS=--max-old-space-size=4096 tsx scripts/scryfall-download.ts",
    "scryfall:transform": "NODE_OPTIONS=--max-old-space-size=4096 tsx src/services/scryfallIngest.ts --from-file data/scryfall-default-cards.json.gz",
    "scryfall:copy": "NODE_OPTIONS=--max-old-space-size=4096 tsx scripts/scryfall-copy.ts",
    "scryfall:refresh": "npm run scryfall:download && npm run scryfall:transform && npm run scryfall:copy",
    "mtg:cleanup": "tsx scripts/cleanup-mtg.ts",
    "searchindex:rebuild": "tsx scripts/reindex-printings.ts",
    "searchindex:audit": "tsx scripts/audit-search-index.ts",
    "db:sanitize-paper-only": "tsx scripts/db-sanitize-paper-only.ts",
    "db:analyze": "tsx scripts/db-analyze.ts",
    "db:verify:set-normalization": "tsx scripts/db-verify-set-normalization.ts",
    "db:migrate": "prisma migrate deploy"
  },
  "dependencies": {
    "@prisma/client": "^6.16.1",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.58.0",
    "next": "15.5.3",
    "pg": "^8.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "stream-json": "^1.7.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "prisma": "^6.16.1",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5",
    "vitest": "^2.1.3"
  }
}
