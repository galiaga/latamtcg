    "date-fns": "^4.1.0",
    "next": "15.5.3",
    "nprogress": "^0.2.0",
    "pg": "^8.12.0",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "recharts": "^3.3.0",
    "stream-json": "^1.7.5",
    "swr": "^2.3.0",
    "zod": "^4.1.12"
