    "db:verify:set-normalization": "tsx scripts/db-verify-set-normalization.ts",
    "db:migrate": "prisma migrate deploy",
    "db:migrate:dev": "prisma migrate dev",
    "db:seed:pricing": "tsx scripts/seed-pricing-config.ts",
    "db:seed:policy": "tsx scripts/seed-store-policy.ts",
    "db:verify:pricing": "tsx scripts/verify-pricing-setup.ts",
    "db:check:limits": "tsx scripts/check-purchase-limits-migration.ts",
