    "scryfall:copy": "NODE_OPTIONS=--max-old-space-size=4096 tsx scripts/scryfall-copy.ts",
    "scryfall:refresh": "npm run scryfall:download && npm run scryfall:transform && npm run scryfall:copy",
    "mtg:cleanup": "tsx scripts/cleanup-mtg.ts",
